--ui
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "xtrey10x hub - RB World 4", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Tab2 = Window:MakeTab({
	Name = "Player",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Tab3 = Window:MakeTab({
	Name = "Teleport",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


--values
_G.autodribble = true
_G.autoGuard = true

--functions

function aimbot()
    local UserInputService = game:GetService("UserInputService")

local function onInputBegan(input, gameProcessed)
if input.KeyCode == Enum.KeyCode.Q then --Can change Y to another Key you want!
local player = game.Players.LocalPlayer
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "Shoot",
    [2] = true
}

game:GetService("ReplicatedStorage").GameEvents.ClientAction:FireServer(unpack(args))
wait(0.86)
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "Shoot",
    [2] = false
}

game:GetService("ReplicatedStorage").GameEvents.ClientAction:FireServer(unpack(args))

end
end


UserInputService.InputBegan:Connect(onInputBegan)
end
function tpball()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Basketball.CFrame
end
function espball()
    for __,v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Model") or v:IsA("Part") or v:IsA("MeshPart") then
        local a = Instance.new("BillboardGui",v)
        a.Size = UDim2.new(1,0, 1,0)
        a.Name = "ball"
        a.AlwaysOnTop = true
        local b = Instance.new("Frame",a)
        b.Size = UDim2.new(1,0, 1,0)
        b.BackgroundTransparency = 1
        b.BorderSizePixel = 0
        local c = Instance.new("TextLabel",b)
        c.Text = v.Name
        c.Size = UDim2.new(1,0, 1,0)
        c.BackgroundTransparency = 1
        c.BorderSizePixel = 0
        end
        end
end
function redeemcodes()
    local args = {
        [1] = "Codes",
        [2] = "#ForgiveMe?"}
    game:GetService("ReplicatedStorage").SocialFunction:InvokeServer(unpack(args))
    wait()
    local args = {
        [1] = "Codes",
        [2] = "SeasonOne"}
    game:GetService("ReplicatedStorage").SocialFunction:InvokeServer(unpack(args))
    wait()
    local args = {
        [1] = "Codes",
        [2] = "#RobloxDown"}
    game:GetService("ReplicatedStorage").SocialFunction:InvokeServer(unpack(args))
    wait()
    local args = {
        [1] = "Codes",
        [2] = "3KKoins"}
    game:GetService("ReplicatedStorage").SocialFunction:InvokeServer(unpack(args))
    wait()
end
function autodribble()
    while _G.autodribble == true do
local args = {
    [1] = "Dribble Move",
    [2] = "Z"}
game:GetService("ReplicatedStorage").GameEvents.ClientAction:FireServer(unpack(args))
wait(1.5)
local args = {
    [1] = "Dribble Move",
    [2] = "C"}
game:GetService("ReplicatedStorage").GameEvents.ClientAction:FireServer(unpack(args))
wait(1.5)
end
end
function autoGuard()
while _G.autoGuard == true do
local args = {
    [1] = "Guard",
    [2] = true}
game:GetService("ReplicatedStorage").GameEvents.ClientAction:FireServer(unpack(args))
wait()
end
end
--buttons

Tab:AddButton({
	Name = " aimbot shot (press Q to Shoot aimbot)",
	Callback = function()
      		print("button pressed")
            aimbot()
  	end    
})
Tab:AddToggle({
	Name = "auto dribble (kinda bugs the aimbot)",
	Default = false,
	Callback = function(Value)
		print(Value)
        _G.autodribble = Value
        autodribble()
	end    
})
Tab:AddToggle({
	Name = "auto guard",
	Default = false,
	Callback = function(Value)
		print(Value)
        _G.autoGuard = Value
        autoGuard()
	end    
})
Tab:AddButton({
	Name = "teleport to ball",
	Callback = function()
      		print("button pressed")
            tpball()

  	end    
})
Tab:AddButton({
	Name = "redeem all codes",
	Callback = function()
      		print("button pressed")
            redeemcodes()
  	end    
})
Tab2:AddSlider({
	Name = "Fov",
	Min = 0,
	Max = 120,
	Default = 77,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "fov range",
	Callback = function(Value)
		print(Value)
        local amount = Value
        game:GetService'Workspace'.Camera.FieldOfView = amount
	end    
})

Tab3:AddButton({
	Name = "Main Menu",
	Callback = function()
      		print("button pressed")
            game:GetService("TeleportService"):Teleport(7899881670, LocalPlayer)
  	end    
})
